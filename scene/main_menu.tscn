[sub_resource type="GDScript" id="GDScript_du31s"]
resource_name = "main_menu"
script/source = "extends Control

@onready var options_menu: TextureRect = $OptionsMenu


func _ready() -> void:
	options_menu.hide()


func _on_options_button_pressed() -> void:
	options_menu.show()


func _on_exit_options_button_pressed() -> void:
	options_menu.hide()


func _on_play_button_pressed() -> void:
	get_tree().change_scene_to_file(\"uid://cglc5tcgjld28\")


func _on_exit_button_pressed() -> void:
	get_tree().quit()
"

